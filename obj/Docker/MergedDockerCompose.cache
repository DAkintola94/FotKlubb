name: dockercompose2702444270072307336
services:
  fotklubb:
    build:
      context: C:\Users\Eier\source\repos\FotKlubb
      dockerfile: FotKlubb/Dockerfile
    depends_on:
      mariadb:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
      connectionStrings__DefaultConnection: Server=mariadb;Database=FootyBase;User=root;Password=Testingtesting1234
    image: fotklubb
    networks:
      default: null
    ports:
      - mode: ingress
        target: 80
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Eier\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Eier\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  mariadb:
    container_name: mariadb
    environment:
      MYSQL_DATABASE: Websitebase
      MYSQL_PASSWORD: Testingtesting1234
      MYSQL_ROOT_PASSWORD: Testingtesting1234
      MYSQL_USER: root
    image: mariadb:latest
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3306
        published: "3306"
        protocol: tcp
    volumes:
      - type: volume
        source: mariadb_data
        target: /var/lib/mysql
        volume: {}
networks:
  default:
    name: dockercompose2702444270072307336_default
volumes:
  mariadb_data:
    name: dockercompose2702444270072307336_mariadb_data